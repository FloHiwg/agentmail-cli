{
  "name": "openclaw-agentmail-cli",
  "version": "1.0.0",
  "description": "CLI for AgentMail - Email accounts for AI agents",
  "type": "module",
  "main": "./src/cli.js",
  "bin": {
    "agentmail": "./bin/agentmail.js",
    "agentmail-mcp": "./bin/agentmail-mcp.js"
  },
  "files": [
    "bin/",
    "src/",
    "config/",
    "skills/",
    "README.md"
  ],
  "scripts": {
    "start": "node bin/agentmail.js",
    "start:mcp": "node bin/agentmail-mcp.js",
    "test": "node --test test/*.test.js",
    "build:mcp": "npx mcporter generate-cli agentmail --output ./dist/agentmail --name agentmail",
    "build:mcp:binary": "npx mcporter generate-cli agentmail --output ./dist/agentmail --compile --name agentmail",
    "list-tools": "npx mcporter list agentmail --all-parameters",
    "test:mcp": "npx mcporter call agentmail.list_inboxes",
    "prepublishOnly": "npm test && node bin/agentmail.js --version"
  },
  "keywords": [
    "agentmail",
    "email",
    "cli",
    "mcp",
    "ai-agents",
    "ai",
    "agent",
    "inbox",
    "mail",
    "automation",
    "openclaw",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://agentmail.to",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FloHiwg/agentmail-cli.git"
  },
  "bugs": {
    "url": "https://github.com/FloHiwg/agentmail-cli/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "agentmail": "^0.3.3",
    "commander": "^14.0.3"
  },
  "optionalDependencies": {
    "mcporter": "^0.7.3"
  },
  "devDependencies": {
    "agentmail-mcp": "^0.2.1"
  }
}
